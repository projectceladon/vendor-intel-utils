From 3b7eeae86486be479bd414cd265dc6693221c0bf Mon Sep 17 00:00:00 2001
From: Tanuj Tekriwal <tanuj.tekriwal@intel.com>
Date: Sun, 13 Sep 2020 23:20:28 +0530
Subject: [PATCH] Check the avb version to 1.1

Tracked-On: None
Signed-off-by: Tanuj Tekriwal <tanuj.tekriwal@intel.com>
---
 RuntimeInfo-target.cpp | 4 ++--
 1 file changed, 2 insertions(+), 2 deletions(-)

diff --git a/RuntimeInfo-target.cpp b/RuntimeInfo-target.cpp
index d221897..1c12c37 100644
--- a/RuntimeInfo-target.cpp
+++ b/RuntimeInfo-target.cpp
@@ -148,11 +148,11 @@ status_t RuntimeInfoFetcher::parseKernelVersion() {
 }
 
 status_t RuntimeInfoFetcher::fetchAvb() {
-    std::string prop = android::base::GetProperty("ro.boot.vbmeta.avb_version", "0.0");
+    std::string prop = android::base::GetProperty("ro.boot.vbmeta.avb_version", "1.1");
     if (!parse(prop, &mRuntimeInfo->mBootVbmetaAvbVersion)) {
         return UNKNOWN_ERROR;
     }
-    prop = android::base::GetProperty("ro.boot.avb_version", "0.0");
+    prop = android::base::GetProperty("ro.boot.avb_version", "1.1");
     if (!parse(prop, &mRuntimeInfo->mBootAvbVersion)) {
         return UNKNOWN_ERROR;
     }
