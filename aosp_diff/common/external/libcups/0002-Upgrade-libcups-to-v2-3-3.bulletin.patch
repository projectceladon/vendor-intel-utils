-        int i;				/* Looping var */
-
-        for (i = 0; i < attr->num_values; i ++)
-        {
-	  if ((childattr = ippFindAttribute(attr->values[i].collection, child, type)) != NULL)
-	  {
-	    attr->values[0].collection->curindex = i;
-	    return (childattr);
-	  }
-        }
-      }
