From 6ab7a5221650e868c2e00a04b2136ce7d2f1c12e Mon Sep 17 00:00:00 2001
From: "Alam, SahibeX" <sahibex.alam@intel.com>
Date: Fri, 22 Dec 2023 08:12:21 +0530
Subject: [PATCH] Update security_patch_level string

Security_patch_level needs to be updated
When ASB Security patches are integrated.
Signed-off-by: Alam, SahibeX <sahibex.alam@intel.com>
---
 core/version_defaults.mk | 2 +-
 1 file changed, 1 insertion(+), 1 deletion(-)

diff --git a/core/version_defaults.mk b/core/version_defaults.mk
index 067c450bd1..226b73b1d7 100644
--- a/core/version_defaults.mk
+++ b/core/version_defaults.mk
@@ -104,7 +104,7 @@ ifndef PLATFORM_SECURITY_PATCH
     #  It must be of the form "YYYY-MM-DD" on production devices.
     #  It must match one of the Android Security Patch Level strings of the Public Security Bulletins.
     #  If there is no $PLATFORM_SECURITY_PATCH set, keep it empty.
-    PLATFORM_SECURITY_PATCH := 2023-12-05
+    PLATFORM_SECURITY_PATCH := 2024-01-01
 endif
 
 include $(BUILD_SYSTEM)/version_util.mk
-- 
