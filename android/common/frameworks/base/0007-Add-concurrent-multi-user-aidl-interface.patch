+package com.android.internal.os;
+
+
+/**
+ * Concurrent multi user interface.
+ *
+ * @hide
+ */
+interface IConcurrentMultiUser {
+    /**
+     * Get user id for different display id
+     */
+    int getUserIdForDisplayId(int displayId);
+
+    /**
+     * Get user id for different display port
+     */
+    int getUserIdForDisplayPort(int displayPort);
+
+    /**
+     * Get display id for different display port
+     */
+    int getDisplayIdForDisplayPort(int displayPort);
+
+    /**
+     * Start user with specific user id
+     */
+    boolean startUser(int userId);
+
+    /**
+     * Stop user with specific user id
+     */
+    boolean stopUser(int userId);
+}
