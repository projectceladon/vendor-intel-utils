From 50c0adb6cca9323a889af60cb0a33e734ca8b284 Mon Sep 17 00:00:00 2001
From: sgnanase <sundar.gnanasekaran@intel.com>
Date: Thu, 13 May 2021 15:39:32 +0530
Subject: [PATCH] CIC TGL NPI Patches

Resolve compilation issues

Change-Id: Idb2a6b4150f0bef399f6b5ab0233b1ac2724ad43
Tracked-On: OAM-97081
Signed-off-by: sgnanase <sundar.gnanasekaran@intel.com>
---
 core/config.mk      | 2 +-
 core/deprecation.mk | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/core/config.mk b/core/config.mk
index ff479f6faf..e93222415b 100644
--- a/core/config.mk
+++ b/core/config.mk
@@ -176,7 +176,7 @@ BUILD_AUX_STATIC_LIBRARY :=$= $(BUILD_SYSTEM)/aux_static_library.mk
 BUILD_AUX_EXECUTABLE :=$= $(BUILD_SYSTEM)/aux_executable.mk
 BUILD_SHARED_LIBRARY :=$= $(BUILD_SYSTEM)/shared_library.mk
 BUILD_EXECUTABLE :=$= $(BUILD_SYSTEM)/executable.mk
-BUILD_HOST_EXECUTABLE :=$= $(BUILD_SYSTEM)/host_executable.mk
+BUILD_HOST_EXECUTABLE := $(BUILD_SYSTEM)/host_executable.mk
 BUILD_PACKAGE :=$= $(BUILD_SYSTEM)/package.mk
 BUILD_PHONY_PACKAGE :=$= $(BUILD_SYSTEM)/phony_package.mk
 BUILD_RRO_PACKAGE :=$= $(BUILD_SYSTEM)/build_rro_package.mk
diff --git a/core/deprecation.mk b/core/deprecation.mk
index 19bd4cfb94..8acb44c4fe 100644
--- a/core/deprecation.mk
+++ b/core/deprecation.mk
@@ -18,6 +18,7 @@ AVAILABLE_BUILD_MODULE_TYPES :=$= \
   BUILD_SHARED_LIBRARY \
   BUILD_STATIC_JAVA_LIBRARY \
   BUILD_STATIC_LIBRARY \
+  BUILD_HOST_EXECUTABLE \
 
 # These are BUILD_* variables that will throw a warning when used. This is
 # generally a temporary state until all the devices are marked with the
@@ -32,7 +33,6 @@ DEFAULT_ERROR_BUILD_MODULE_TYPES :=$= \
   BUILD_AUX_EXECUTABLE \
   BUILD_AUX_STATIC_LIBRARY \
   BUILD_COPY_HEADERS \
-  BUILD_HOST_EXECUTABLE \
   BUILD_HOST_FUZZ_TEST \
   BUILD_HOST_NATIVE_TEST \
   BUILD_HOST_SHARED_LIBRARY \
