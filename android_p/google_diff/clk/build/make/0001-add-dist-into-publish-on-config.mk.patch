From 0d8b2e77d8b72f672781f014562c0b01658b872e Mon Sep 17 00:00:00 2001
From: chen yangyang <yangyangx.chen@intel.com>
Date: Thu, 17 Oct 2019 09:37:44 +0000
Subject: [PATCH] add dist into publish on config.mk

Tracked-On: OAM-88214
Signed-off-by: chen yangyang <yangyangx.chen@intel.com>
---
 core/config.mk | 4 +++-
 1 file changed, 3 insertions(+), 1 deletion(-)

diff --git a/core/config.mk b/core/config.mk
index d218408..4561305 100644
--- a/core/config.mk
+++ b/core/config.mk
@@ -86,7 +86,9 @@ FORCE:
 
 ORIGINAL_MAKECMDGOALS := $(MAKECMDGOALS)
 
-dist_goal := $(strip $(filter dist,$(MAKECMDGOALS)))
+dist_goal := $(strip $(filter dist publish,$(MAKECMDGOALS)))
+$(if $(dist_goal), $(eval dist_goal = dist))
+$(warning $(dist_goal))
 MAKECMDGOALS := $(strip $(filter-out dist,$(MAKECMDGOALS)))
 
 UNAME := $(shell uname -sm)
-- 
