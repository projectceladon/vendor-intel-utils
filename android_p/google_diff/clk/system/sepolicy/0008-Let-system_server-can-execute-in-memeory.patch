 private/system_server.te | 1 -
 1 file changed, 1 deletion(-)

diff --git a/private/system_server.te b/private/system_server.te
index b037fe4a..3ce49306 100644
--- a/private/system_server.te
+++ b/private/system_server.te
@@ -846,7 +846,6 @@ neverallow system_server data_file_type:file no_x_file_perms;
 neverallow system_server { dev_type -frp_block_device }:blk_file no_rw_file_perms;
 
 # system_server should never use JIT functionality
-neverallow system_server self:process execmem;
 neverallow system_server ashmem_device:chr_file execute;
 
