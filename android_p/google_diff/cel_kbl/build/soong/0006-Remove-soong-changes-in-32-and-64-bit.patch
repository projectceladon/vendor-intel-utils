From 616737600d076565839183dafa5697e27afae29e Mon Sep 17 00:00:00 2001
From: anuvarsh <anuvarshini.bc@intel.com>
Date: Thu, 12 Sep 2019 14:43:36 +0530
Subject: [PATCH] Remove soong changes in 32 and 64 bit

Tracked-On:
Signed-off-by: anuvarsh <anuvarshini.bc@intel.com>
---
 cc/config/x86_64_device.go | 4 ++--
 cc/config/x86_device.go    | 4 ++--
 2 files changed, 4 insertions(+), 4 deletions(-)

diff --git a/cc/config/x86_64_device.go b/cc/config/x86_64_device.go
index 6d0e4e22..ba1aaff1 100644
--- a/cc/config/x86_64_device.go
+++ b/cc/config/x86_64_device.go
@@ -57,8 +57,8 @@ var (
 		"sse4_1": []string{"-msse4.1"},
 		"sse4_2": []string{"-msse4.2"},
 		"popcnt": []string{"-mpopcnt"},
-		"avx":    []string{"-mavx"},
-		"avx2":   []string{"-mavx2"},
+		"avx":    []string{""},
+		"avx2":   []string{""},
 		"aes_ni": []string{"-maes"},
 	}
 )
diff --git a/cc/config/x86_device.go b/cc/config/x86_device.go
index aca9e14a..3cbc04fd 100644
--- a/cc/config/x86_device.go
+++ b/cc/config/x86_device.go
@@ -75,8 +75,8 @@ var (
 		"sse4":   []string{"-msse4"},
 		"sse4_1": []string{"-msse4.1"},
 		"sse4_2": []string{"-msse4.2"},
-		"avx":    []string{"-mavx"},
-                "avx2":   []string{"-mavx2"},
+		"avx":    []string{},
+                "avx2":   []string{},
 		"aes_ni": []string{"-maes"},
 	}
 )
-- 
