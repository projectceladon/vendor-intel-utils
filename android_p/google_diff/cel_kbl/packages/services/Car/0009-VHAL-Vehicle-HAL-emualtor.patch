+def rxThread(self, v):
+    while(1):
+        msg = v.rxMsg()
+        if (msg.msg_type == VehicleHalProto_pb2.SET_PROPERTY_RESP):
+            if msg.status == 0:
+                print ("Success", str(msg.status))
+                self.lineEdit.setText("Success:" +str(msg.status))
+            else:
+                print ("Error:", str(msg.status))
+                self.lineEdit.setText("Error:" +str(msg.status))
+        else:
+            print ("msg: ", msg);
