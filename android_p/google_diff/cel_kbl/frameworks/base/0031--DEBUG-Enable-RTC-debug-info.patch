From 91e22cdf9fdb1a9109e8e62ae031b5516f09a931 Mon Sep 17 00:00:00 2001
From: "Wang, ArvinX" <arvinx.wang@intel.com>
Date: Wed, 24 Jul 2019 18:00:37 +0800
Subject: [PATCH] [DEBUG] Enable RTC debug info

Tracked-On: https://jira.devtools.intel.com/browse/OAM-
Signed-off-by: Wang, ArvinX <arvinx.wang@intel.com>
---
 services/core/java/com/android/server/AlarmManagerService.java | 2 +-
 services/core/jni/com_android_server_AlarmManagerService.cpp   | 2 +-
 2 files changed, 2 insertions(+), 2 deletions(-)

diff --git a/services/core/java/com/android/server/AlarmManagerService.java b/services/core/java/com/android/server/AlarmManagerService.java
index f79a51b13af..502290193be 100644
--- a/services/core/java/com/android/server/AlarmManagerService.java
+++ b/services/core/java/com/android/server/AlarmManagerService.java
@@ -128,7 +128,7 @@ class AlarmManagerService extends SystemService {
     static final int TYPE_NONWAKEUP_MASK = 0x1; // low bit => non-wakeup
 
     static final String TAG = "AlarmManager";
-    static final boolean localLOGV = false;
+    static final boolean localLOGV = true;
     static final boolean DEBUG_BATCH = localLOGV || false;
     static final boolean DEBUG_VALIDATE = localLOGV || false;
     static final boolean DEBUG_ALARM_CLOCK = localLOGV || false;
diff --git a/services/core/jni/com_android_server_AlarmManagerService.cpp b/services/core/jni/com_android_server_AlarmManagerService.cpp
index 47350c11f95..51423eae1ea 100644
--- a/services/core/jni/com_android_server_AlarmManagerService.cpp
+++ b/services/core/jni/com_android_server_AlarmManagerService.cpp
@@ -16,7 +16,7 @@
 */
 
 #define LOG_TAG "AlarmManagerService"
-
+#define LOG_NDEBUG 0
 #include <nativehelper/JNIHelp.h>
 #include "jni.h"
 #include <utils/Log.h>
