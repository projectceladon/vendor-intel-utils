From 74968c8243518ac4f24fefddd72b76db33417248 Mon Sep 17 00:00:00 2001
From: li zhuangzhi <zhuangzhi.li@intel.com>
Date: Fri, 6 Sep 2019 19:22:33 +0800
Subject: [PATCH] Add suspend mixins group

Tracked-On:
Signed-off-by: li zhuangzhi <zhuangzhi.li@intel.com>
---
 groups/suspend/default            | 1 +
 groups/suspend/false/empty_dir    | 0
 groups/suspend/never/init.rc      | 2 ++
 groups/suspend/no_console/init.rc | 2 ++
 4 files changed, 5 insertions(+)
 create mode 120000 groups/suspend/default
 create mode 100644 groups/suspend/false/empty_dir
 create mode 100644 groups/suspend/never/init.rc
 create mode 100644 groups/suspend/no_console/init.rc

diff --git a/groups/suspend/default b/groups/suspend/default
new file mode 120000
index 0000000..02e4a84
--- /dev/null
+++ b/groups/suspend/default
@@ -0,0 +1 @@
+false
\ No newline at end of file
diff --git a/groups/suspend/false/empty_dir b/groups/suspend/false/empty_dir
new file mode 100644
index 0000000..e69de29
diff --git a/groups/suspend/never/init.rc b/groups/suspend/never/init.rc
new file mode 100644
index 0000000..1609f74
--- /dev/null
+++ b/groups/suspend/never/init.rc
@@ -0,0 +1,2 @@
+on boot
+    write /sys/power/wake_lock nosleeptillbrooklyn
diff --git a/groups/suspend/no_console/init.rc b/groups/suspend/no_console/init.rc
new file mode 100644
index 0000000..3f9bce2
--- /dev/null
+++ b/groups/suspend/no_console/init.rc
@@ -0,0 +1,2 @@
+on early-init
+    write /sys/module/printk/parameters/console_suspend N
-- 
2.7.4

